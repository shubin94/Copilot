# Production testing environment (template)
# For actual production deployment, only set NODE_ENV and DATABASE_URL in your hosting environment
# All other secrets should be in the app_secrets table

NODE_ENV=production
DATABASE_URL=postgresql://postgres:<PASSWORD>@localhost:5432/askdetective_v2
# DB_ALLOW_INSECURE_DEV should be false in production (or unset to default to false)
# Only set to true for development/testing
# DB_ALLOW_INSECURE_DEV=false
PORT=5000

# Set locally or via a developer-provided .env.example (never commit real keys)
SUPABASE_URL=YOUR_SUPABASE_URL
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
S3_ENDPOINT=http://127.0.0.1:54321/storage/v1/s3
